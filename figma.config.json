{
  "codeConnect": {
    "include": ["src/ui/**/*", "src/figma/**/*"],
    "exclude": ["src/figma/icons/*"],
    "parser": "react",
    "importPaths": {
      "ui/compositions/**/*": "compositions",
      "ui/icons/*": "icons",
      "ui/images/*": "images",
      "ui/layout/**/*": "layout",
      "ui/primitives/**/*": "primitives"
    },
    "paths": {
      "compositions": ["src/ui/compositions"],
      "hooks": ["src/ui/hooks"],
      "icons": ["src/ui/icons"],
      "images": ["src/ui/images"],
      "layout": ["src/ui/layout"],
      "primitives": ["src/ui/primitives"],
      "providers": ["src/ui/providers"],
      "utils": ["src/ui/utils"]
    }
  }
}
